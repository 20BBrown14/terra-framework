  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cerner/terra-framework/issues"
  },
  "homepage": "https://github.com/cerner/terra-framework#readme",
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "terra-base": "^3.1.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.5.8",
    "resize-observer-polyfill": "^1.4.1",
    "terra-content-container": "^2.18.0",
